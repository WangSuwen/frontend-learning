    <!---pag 3 检测数据 ---->
    <%
        const resultTable = [];
        resultTable.push({
            "addressDesc": "v",
            "distance": {
                "h": '',
                "v": ''
            },
            "device": {
                "type": '',
                "number":''
            },
            "applications": [],
            "operator": [
                {},
                {},
                {}
            ]
        });
        const resultTables = resultTable.reduce((t, c, index)=>{
            c.serialIndex = index+1;
            if(t.length===0){
                t.push([c]);
            }else{
                const last = t[t.length-1];
                const lastLength = last.reduce((t1, c1)=>t1+c1.operator.length, 0);
                if(lastLength+c.operator.length<=17){
                    last.push(c);
                }else{
                    t.push([c]);
                }
            }
            return t;
        }, []);
    %>
    <% resultTables.forEach(resultTable=>{%>
        <div class="landscape-parent">
            <div class="landscape">
                <div class="center" style="font-size: 22pt;letter-spacing: 10pt;">检测报告</div>
                <p class="fz-13" style="margin-bottom: 10pt;">报告编号：<%= reportNo %></p>
                <table class="border-table content-center project-table" style="width: 100%;">
                    <thead>
                        <tr><td colspan="11" style="background-color: #ccc;text-align:center;font-size:12pt;">检测结果</td></tr>
                        <tr>
                            <td rowspan="2" style="width: 20pt">序号</td>
                            <td rowspan="2">检测点位描述</td>
                            <td colspan="2" style="width: 20%">与天线的距离（m）</td>
                            <td colspan="2" style="width: 20%">发射天线</td>
                            <td colspan="2" style="width: 14%">5G终端设备</td>
                            <td rowspan="2" style="width: 10%">应用场景</td>
                            <td rowspan="2" style="width: 10%"><div>功率密度</div>（μW/c㎡）</td>
                            <td rowspan="2" style="width: 10%"><div>占标比</div>（%）</td>
                        </tr>
                        <tr>
                            <td style="width: 10%">垂直</td>
                            <td style="width: 10%">水平</td>
                            <td style="width: 7%">运营商</td>
                            <td style="width: 13%">下行频段（MHz）</td>
                            <td style="width: 9%">型号</td>
                            <td style="width: 5%">数量</td>
                        </tr>
                    </thead>
                    <tbody>
                    <!----循环输出数据-->
                    <% resultTable.forEach(function(item, idx){ %>
                        <% item.operator.forEach(function(el, i){ %>
                            <% if ( i === 0 ){ %>
                            <tr>
                                <td rowspan="<%= item.operator.length %>" style="vertical-align: middle;"><%= item.serialIndex %></td> <!----序号-->
                                <td rowspan="<%= item.operator.length %>" style="vertical-align: middle;"><%= item.addressDesc %></td> <!----点位描述-->
                                <td rowspan="<%= item.operator.length %>" style="vertical-align: middle;"><%= item.distance.v %></td> <!----垂直-->
                                <td rowspan="<%= item.operator.length %>" style="vertical-align: middle;"><%= item.distance.h %></td> <!----水平-->
                                <td><%= el.name %></td> <!----运营商-->
                                <td><%= el.downloadVolume %></td> <!----下行频率-->
                                <% if ( idx === 0 ){ %>
                                    <td rowspan="<%= resultTableLength %>" style="vertical-align: middle;"><%= item.device.type %></td> <!----型号-->
                                    <td rowspan="<%= resultTableLength %>" style="vertical-align: middle;"><%= item.device.number %></td> <!----数量-->
                                    <td rowspan="<%= resultTableLength %>" style="vertical-align: middle;">
                                        <div>
                                            <% item.applications.forEach(function(ell){ %>
                                                <div><%= ell %></div>
                                            <% }) %>
                                        </div>
                                    </td> <!----应用场景-->
                                <% } %>
                                <td><%= el.powerVolume %></td> <!----功率密度-->
                                <td><%= el.occupancyRatio %></td> <!----占标比-->
                            </tr>
                            <% } else { %>
                            <tr>
                                <td><%= el.name %></td> <!----运营商-->
                                <td><%= el.downloadVolume %></td> <!----下行频率-->
                                <td><%= el.powerVolume %></td> <!----功率密度-->
                                <td><%= el.occupancyRatio %></td> <!----占标比-->
                            </tr>
                            <% }; %>
                        <% }) %>
                    <% }); %>
                    <tr>
                        <td colspan="3">
                            <p style="text-decoration: underline;">以下正文空白</p>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    <%})%>

